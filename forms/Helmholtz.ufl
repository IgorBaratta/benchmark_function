from ufl import *

element = FiniteElement("Lagrange", $cell, $degree)
mesh = Mesh(VectorElement("Lagrange", $cell, 1))

V = FunctionSpace(mesh, element)
u = TrialFunction(V)
v = TestFunction(V)

W = FunctionSpace(mesh, FiniteElement("Lagrange", $cell, 1))
k = Coefficient(W)

a = inner(grad(u), grad(v))*dx + k*inner(u, v)*dx

un = Coefficient(V)
L = action(a, un)
from ufl import *

element = VectorElement("Lagrange", tetrahedron, $degree)
mesh = Mesh(VectorElement("Lagrange", tetrahedron, 1))

V = FunctionSpace(mesh, element)

u = TrialFunction(V)
v = TestFunction(V)

W = FunctionSpace(mesh, FiniteElement("Lagrange", tetrahedron, 1))
k = Coefficient(W)

def eps(v):
    return sym(grad(v))

a = k*inner(eps(u), eps(v))*dx

un = Coefficient(V)
L = action(a, un)
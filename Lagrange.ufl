from ufl import *

element = FiniteElement("Lagrange", tetrahedron, $degree)
mesh = Mesh(VectorElement("Lagrange", tetrahedron, $gdegree))

V = FunctionSpace(mesh, element)
u = TrialFunction(V)
v = TestFunction(V)

W = FunctionSpace(mesh, FiniteElement("Lagrange", tetrahedron, degree))
k = Coefficient(W)

a = k*inner(grad(u), grad(v))*dx